search\" placeholder=\"Search Post\">\r\n     <table width=\"70%\" style=\"margin: auto\" border=\"1\">\r\n      <thead>\r\n         <tr>\r\n             <th>SL</th>\r\n             <th>User ID</th>\r\n             <th>Title</th>\r\n             <th>Body</th>\r\n         </tr>\r\n      </thead>\r\n      <tbody>\r\n         <tr v-for=\"(post, index) in postsSearch\" :key=\"index\">\r\n             <td>{{ index + 1 }}</td>\r\n             <td>{{ post.userId }}</td>\r\n             <td>{{ post.title }}</td>\r\n             <td>{{ post.body | snipper }}</td>\r\n         </tr>\r\n      </tbody>\r\n      </table>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: \"Hook\",\r\n  data(){\r\n    return {\r\n      posts : [],\r\n      search: ''\r\n\r\n    }\r\n  },\r\n  computed:{\r\n    postsSearch(){\r\n      return this.posts.filter(post => {\r\n        return post.title.match(this.search)\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.allPosts()\r\n },\r\n  methods:{\r\n    allPosts(){\r\n      axios.get('https://jsonplaceholder.typicode.com/posts')\r\n          .then(res => {\r\n            // console.log(res.data)\r\n            return this.posts = res.data;\r\n          }).catch(error => {\r\n            console.log(error)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}